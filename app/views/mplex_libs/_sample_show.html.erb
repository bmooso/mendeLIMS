<table class="list" style="margin-bottom:0;">
  <tr>	
  	<th>Index Tag</th>
	<th>Tag Seq</th>
  	<th>Runtype/Adapter</th>
	<th>Sample Name</th>
	<th>Source DNA/RNA</th>
	<th>Notes</th>
  </tr>
  <% for lib_sample in @seq_lib.lib_samples %>
    <tr>
	  <td><%=h lib_sample.index_tags %></td>
	  <td><%=h IndexTag.find_or_blank(lib_sample.multiplex_type, lib_sample.index_tags)%></td>
      <td><%=h lib_sample.multiplex_type %></td>
	  <td><%=h lib_sample.sample_name %></td>
	  <% if lib_sample.processed_sample %>
	  <td><%=link_to lib_sample.source_DNA, lib_sample.processed_sample %></td>
	  <% else %>
	  <td><%=h lib_sample.source_DNA %></td>
	  <% end %>
	  <td><%=h lib_sample.notes %></td>
    </tr>
    <% end %>
</table>
