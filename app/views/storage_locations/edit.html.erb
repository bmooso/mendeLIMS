<h1>Edit storage location</h1>

<% tagged_form_for(@storage_location) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'storage_loc_form', :locals => {:f => f} %>
    
  <%= break_clear(f.submit "Update") -%>&nbsp;&nbsp;<%= link_to 'Back', storage_locations_path %>
<% end %>


