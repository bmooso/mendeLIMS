<p>See below for new clinical sample:</p>

<table>
  <tr>
    <th>MRN</th>
	<th>Patient ID</th>
    <th>Gender</th>
	<th>Ethnicity</th>
    <th>Race</th>
	<th>Collection Date</th>
	<th>Clinic</th>
	<th>Consent Protocol</th>
  </tr>
  <tr>
  	<td><%=h @mrn %></td>
	<td><%=h @sample_characteristic.patient_id %></td>
	<td><%=h @sample_characteristic.gender %></td>
	<td><%=h @sample_characteristic.ethnicity %></td>
	<td><%=h @sample_characteristic.race %></td>
	<td><%=h @sample_characteristic.collection_date %></td>
	<td><%=h @sample_characteristic.clinic_or_location %></td>
	<td><%=h @sample_characteristic.consent_nr %></td>
  </tr>
  
  <tr><td>&nbsp;</td></tr>
  
  <tr>
	<th>Sample Site</th>
	<th>Sample Type</th>
	<th>Preservation</th>
	<th>Entered by</th>
	<th>Comments</th>
  </tr>
  <tr>
	<td><%=h @sample_characteristic.sample_tissue %></td>
	<td><%=h @sample_characteristic.sample_type %></td>
	<td><%=h @sample_characteristic.tissue_preservation %></td>
	<td><%=h @upd_by %></td>
	<td><%=h @sample_characteristic.comments %></td>
  </tr>
  
  <tr><td>&nbsp;</td></tr>
  
  <tr>
  	<th>Sample barcode</th>
	<th>Amount</th>
	<th>UOM</th>
	<th>Location</th>
  </tr>
  
  <% for sample in @sample_characteristic.samples %>
  <tr>
    <td><%=h sample.barcode_key %></td>
	<td><%=h sample.amount_initial %></td>
	<td><%=h sample.amount_uom %></td>
	<td><%=h sample.storage_boxbin %></td>
  </tr>	
  <% end %>
</table> 